{
	"name": "vscode-maximize-fix",
	"displayName": "Maximize Window Fix",
	"description": "Fixes issue with the maximized window being larger than the screen on some computers with mixed monitor DPIs.",
	"repository": {
		"type": "git",
		"url": "https://github.com/danielgjackson/vscode-maximize-fix.git"
	},
	"publisher": "danielgjackson",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.47.0"
	},
	"icon": "icon.png",
	"keywords": [
		"maximize",
		"window",
		"fix"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:vscode-maximize-fix.fix"
	],
	"extensionKind": [
		"ui"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-maximize-fix.fix",
				"title": "Maximize Window Fix"
			}
		],
		"keybindings": [
			{
				"command": "vscode-maximize-fix.fix",
				"key": "ctrl+alt+shift+m"
			}
		],
		"configuration": {
			"title": "Maximize Window Fix"
		}
	},
	"scripts": {
		"lint": "eslint .",
		"vscode:prepublish": "build.cmd"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
